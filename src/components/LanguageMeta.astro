---
const { supportedLangs, defaultLang } = Astro.props;
---

<link rel="alternate" href="https://escvienna2026.com/" hreflang="x-default" />
{
  supportedLangs.map((code) => {
    const path = code === defaultLang ? "/" : `/${code}/`;
    const fullPath = "https://escvienna2026.com" + path;
    return <link rel="alternate" href={fullPath} hreflang={code} />;
  })
}
